<p id="notice"><%= notice %></p>

<h1>Place Ratings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @place_ratings.each do |place_rating| %>
      <tr>
        <td><%= link_to 'Show', place_rating %></td>
        <td><%= link_to 'Edit', edit_place_rating_path(place_rating) %></td>
        <td><%= link_to 'Destroy', place_rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place Rating', new_place_rating_path %>
