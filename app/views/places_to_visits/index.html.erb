<p id="notice"><%= notice %></p>

<h1>Places To Visits</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @places_to_visits.each do |places_to_visit| %>
      <tr>
        <td><%= link_to 'Show', places_to_visit %></td>
        <td><%= link_to 'Edit', edit_places_to_visit_path(places_to_visit) %></td>
        <td><%= link_to 'Destroy', places_to_visit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Places To Visit', new_places_to_visit_path %>
