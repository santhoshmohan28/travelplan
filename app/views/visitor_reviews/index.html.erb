<p id="notice"><%= notice %></p>

<h1>Visitor Reviews</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visitor_reviews.each do |visitor_review| %>
      <tr>
        <td><%= link_to 'Show', visitor_review %></td>
        <td><%= link_to 'Edit', edit_visitor_review_path(visitor_review) %></td>
        <td><%= link_to 'Destroy', visitor_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visitor Review', new_visitor_review_path %>
